<div class="brackets">
  <div class="round">
    <% for i in 0..7%>
    <%= render partial:'tournaments/bracket-partials/match', locals: {stage: i, tournament: @tournament} %>
      <% end %>
  </div>
  <div class="lines">
    <% for i in 0..3%>
      <%= render partial:'tournaments/bracket-partials/lines', locals: {height: 'height:320px'} %>
        <% end %>
  </div>
  <div class="round">
    <% for i in 8..11%>
    <%= render partial:'tournaments/bracket-partials/match', locals: {stage: i, tournament: @tournament} %>
      <% end %>
  </div>
  <div class="lines">
    <% for i in 0..1%>
      <%= render partial:'tournaments/bracket-partials/lines', locals: {height: 'height:500px'} %>
        <% end %>
  </div>
  <div class="round">
    <% for i in 12..13%>
    <%= render partial:'tournaments/bracket-partials/match', locals: {stage: i, tournament: @tournament} %>
      <% end %>
  </div>
  <div class="lines">
    <%= render partial:'tournaments/bracket-partials/lines', locals: {height: 'height:860px'} %>
  </div>
  <div class="round">
      <%= render partial:'tournaments/bracket-partials/match', locals: {stage: 14, tournament: @tournament} %>
  </div>
</div>